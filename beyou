DROP TABLE IF EXISTS `user_answers`;
/*!40101 SET @saved_cs_client     = @@character_set_client */;
/*!50503 SET character_set_client = utf8mb4 */;
CREATE TABLE `user_answers` (
  `id` bigint unsigned NOT NULL AUTO_INCREMENT,
  `user_id` int NOT NULL,
  `genre_value` varchar(191) COLLATE utf8mb4_unicode_ci NOT NULL,
  `option_num` int DEFAULT NULL,
  `big_question_id` int NOT NULL,
  `question_num` int NOT NULL,
  `user_answer` varchar(255) COLLATE utf8mb4_unicode_ci DEFAULT '',
  `result` varchar(191) COLLATE utf8mb4_unicode_ci NOT NULL,
  `created_at` time NOT NULL DEFAULT (now()),
  `created_date` date NOT NULL DEFAULT (curdate()),
  `updated_at` timestamp NULL DEFAULT NULL,
  PRIMARY KEY (`id`)
) ENGINE=InnoDB AUTO_INCREMENT=735 DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_unicode_ci;
